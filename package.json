{
	"name": "ng-universal-demo",
	"version": "1.0.0",
	"main": "index.js",
	"repository": {},
	"scripts": {
		"start": "npm run clean && npm run build && npm run server",
		"dev": "npm run clean && npm run build && (npm run watch & npm run server:dev)",
		"build": "npm run build:aot && npm run build:webpack",
		"build:webpack": "webpack -p",
		"build:aot": "ngc",
		"clean": "npm run clean:dist && npm run clean:build",
		"clean:build": "npm run clean:build:webpack && npm run clean:build:ng",
		"clean:build:webpack": "rimraf build",
		"clean:build:ng": "rimraf src/aot/node_modules && rimraf src/aot/src",
		"clean:dist": "rimraf dist",
		"watch": "webpack -p --watch",
		"server": "node dist/server/main.js",
		"server:dev": "nodemon --watch dist dist/server/main.js"
	},
	"engines": {
		"node": ">=7.0.0"
	},
	"license": "MIT",
	"dependencies": {
		"@angular/animations": "4.2.5",
		"@angular/common": "4.2.5",
		"@angular/compiler": "4.2.5",
		"@angular/compiler-cli": "4.2.5",
		"@angular/core": "4.2.5",
		"@angular/http": "4.2.5",
		"@angular/platform-browser": "4.2.5",
		"@angular/platform-server": "4.2.5",
		"@angular/router": "4.2.5",
		"@ngtools/webpack": "1.5.0",
		"@types/express": "4.0.36",
		"@types/node": "8.0.7",
		"express": "4.15.3",
		"nodemon": "1.11.0",
		"raw-loader": "0.5.1",
		"rimraf": "2.6.1",
		"rxjs": "5.4.1",
		"typescript": "2.4.1",
		"webpack": "3.0.0",
		"zone.js": "0.8.12"
	}
}
