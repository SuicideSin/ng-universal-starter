{
	"name": "ng-universal-demo",
	"version": "1.0.0",
	"main": "index.js",
	"repository": {},
	"scripts": {
		"start": "npm run clean && npm run build && npm run server",
		"dev": "npm run clean && npm run watch",
		"build": "npm run build:webpack",
		"build:webpack": "webpack --progress",
		"build:aot": "ngc",
		"clean": "npm run clean:dist && npm run clean:build",
		"clean:build": "npm run clean:build:webpack && npm run clean:build:aot",
		"clean:build:webpack": "rimraf build",
		"clean:build:aot": "rimraf src/aot/node_modules && rimraf src/aot/src",
		"clean:dist": "rimraf dist",
		"watch": "cross-env NODE_ENV=development webpack --progress --watch",
		"server": "cross-env NODE_ENV=production node dist/server/main.js",
		"server:dev": "livereload 'dist/server/main.js, dist/client/main.js' -w 1000 & cross-env NODE_ENV=development nodemon --watch dist dist/server/main.js"
	},
	"engines": {
		"node": ">=7.0.0"
	},
	"license": "MIT",
	"dependencies": {
		"@angular/animations": "4.4.3",
		"@angular/common": "4.4.3",
		"@angular/compiler": "4.4.3",
		"@angular/compiler-cli": "4.4.3",
		"@angular/core": "4.4.3",
		"@angular/forms": "4.4.3",
		"@angular/http": "4.4.3",
		"@angular/platform-browser": "4.4.3",
		"@angular/platform-server": "4.4.3",
		"@angular/router": "4.4.3",
		"@ngtools/webpack": "1.7.1",
		"@types/express": "4.0.37",
		"@types/node": "8.0.31",
		"babel-polyfill": "6.26.0",
		"body-parser": "1.18.2",
		"connect-livereload": "0.6.0",
		"cookie-parser": "1.4.3",
		"cross-env": "5.0.5",
		"express": "4.15.5",
		"livereload": "0.6.2",
		"node-sass": "4.5.3",
		"nodemon": "1.12.1",
		"raw-loader": "0.5.1",
		"rimraf": "2.6.2",
		"rxjs": "5.4.3",
		"sass-loader": "6.0.6",
		"source-map-support": "0.4.18",
		"typescript": "2.5.3",
		"webpack": "3.6.0",
		"webpack-shell-plugin": "0.5.0",
		"zone.js": "0.8.17"
	}
}
